const arr = [3, 1, 4, 4, 2, 4, 3, 2, 4, 4];
const count = {};
let maxCount = 0;
let maxItem;
for (let i = 0; i < arr.length; i++) {
    if (!(arr[i] in count)) {
        count[arr[i]] = 0;
    }
    count[arr[i]]++;
    if (count[arr[i]] > maxCount) {
        maxCount = count[arr[i]];
        maxItem = arr[i];
    }
}
console.log(maxItem);
